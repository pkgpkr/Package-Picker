{
  "executionRoleArn": "arn:aws:iam::529432849523:role/terraform-20200413001522063800000002",
  "containerDefinitions": [
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/pkgpkr-web",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "cpu": 2048,
    "memoryReservation": 4096,
    "image": "nginx:latest",
    "essential": true,
    "name": "nginx"
  }
]
,
  "memory": "4096",
  "family": "pkgpkr-ml-task-definition",
